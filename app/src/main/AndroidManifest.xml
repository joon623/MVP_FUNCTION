<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sleep.asleep_functions">

    <permission
        android:name="android.permission.RECORD_AUDIO"
        android:protectionLevel="dangerous" />
    <permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:protectionLevel="dangerous" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Asleep_functions">
        <activity android:name=".view.AudioRecorderSample.SoundChartActivity">

        </activity>
        <activity android:name=".view.CustomWave.AudioTestActivity" />
        <activity android:name=".view.AudioRecorderSample.AudioRecorderActivity" />
        <activity android:name=".view.CustomWave.CustomWaveActivity">

        </activity>
        <activity android:name=".view.Wave.WaveActivity" />
        <activity android:name=".view.MainActivityTwo" />
        <activity android:name=".view.SleepMeasurement.SleepMeasurementActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.Main.MainActivity" />
    </application>

</manifest>